#include<iostream>
using namespace std;
int main()
{
	char ch;
	cout << "ENter a character ";
	cin >> ch;
	int ascii = ch;
	if ((ascii >= 65 && ascii <= 90) || (ascii >= 97 && ascii <= 122))
	{
		if (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U') {
			cout << "Its a vowel";
		}
		else if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
			cout << "Its a vowel";
		}
		else
		{
			cout << "Its a consonant";
		}
	}
	else
		cout << "Invalid character";
	return 0;
}
